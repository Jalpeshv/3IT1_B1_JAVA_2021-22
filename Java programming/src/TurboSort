import java.util.*;
class codechef
{
    public static void main(String[] args)
    {
     Scanner sc = new Scanner(System.in);
     int n = sc.nextInt();
     int num[] = new int[n];
     int temp=0;
     for(int i=0;i<n;i++)
     {
      num[i] = sc.nextInt();
     }
     Arrays.sort(num);
     for(int i=0;i<n;i++)
     {
      System.out.println(num[i]);
     }
    }
}
