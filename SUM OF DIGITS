import java.util.*;

class Codechef
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        while(t-->0)
        {
            int N=sc.nextInt();
            int Sum=0;
            
            while(N>0)
            {
                Sum+=N%10;
                N/=10;
            }
            System.out.println(Sum);
        }
    }
}
    
