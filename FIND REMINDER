import java.util.*;

class Codechef
{
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        
        while(t-->0)
        {
            int num=sc.nextInt();
            System.out.println(getReverse(num));
            
        }
    }
    public static int getReverse(int num)
    {
        int rev=0;
        while(num!=0)
        {
            int rem=num%10;
            rev=rev*10+rem;
            num=num/10;
        }
        return rev;
    }
}
